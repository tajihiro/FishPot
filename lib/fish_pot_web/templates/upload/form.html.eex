
<%= form_for @changeset, @action, [multipart: true], fn f -> %>
    <div class="form-group">
        <label>Photo</label>
        <%= file_input f, :photo, class: "form-control" %>
    </div>

    <div class="form-group">
        <%= submit "Submit", class: "btn btn-primary" %>
    </div>
<% end %>