<h2>Show Company</h2>

<ul>

  <li>
    <strong>Company name:</strong>
    <%= @company.company_name %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @company.url %>
  </li>

</ul>

<span><%= link "Edit", to: company_path(@conn, :edit, @company) %></span>
<span><%= link "Back", to: company_path(@conn, :index) %></span>
